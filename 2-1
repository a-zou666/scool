#include<bits/stdc++.h>
using namespace std;
int n,p,flag=0,ans;
int lowbit(int x)  // 返回末尾的1
{
    int r=0;
    while(x){
        r++;
        x-=x&-x;
    }
    return r;
}
int main()
{
    cin>>n>>p;
    for(int i=1;;i++){
        if(n-i*p<i) break;
        if(lowbit(n-i*p)>i) continue;
        ans=i;
        flag=1;
        break;
    }
    if(flag) cout<<ans;
    else cout<<"-1";
    return 0;
}
