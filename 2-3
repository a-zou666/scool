#include <iostream>


using namespace std;

const int N = 110;

int T, n, m;
int du[N];

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);

    cin >> T;

    while(T--)
    {
        cin >> n;

        for(int i = n; i >= 1; i--) cout << i << " ";
        cout << "\n";
    }

    return 0;
}
