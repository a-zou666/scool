#include<bits/stdc++.h>
using namespace std;
typedef  long long ll;

ll x,y,z,a,b,c;

void solve(){
    cin>>x>>y>>z>>a>>b>>c;
    ll ans=x*a;
    ans=max(ans,y*b);
    ans=max(ans,z*c);
    cout<<ans<<"\n";
}
signed main(){
    ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    ll t;cin>>t;
    while(t--){
        solve();
    }
    
}
