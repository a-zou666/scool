#include<iostream>
using namespace std;
int main(){
    int n,t;
    cin>>t;
    while(t--){
        cin>>n;
        if(n==3)  cout<<"-1"<<"\n";
        else if(n==2) cout<<"2 1"<<"\n";
        else{
            cout<<n-1<<" "<<n<<" ";
            int a=n-2;
            if(a==0) cout<<"\n";
            else{
                for(int i=1;i<=a;i++) cout<<i<<" ";
                cout<<"\n";
            }
        }
    }
    return 0;
}
